{"ast":null,"code":"import { mapActions } from 'vuex';\nexport default {\n  name: 'EditCtg',\n  data() {\n    return {\n      status: '',\n      message: '',\n      active: false,\n      viewModal: false\n    };\n  },\n  computed: {\n    added() {\n      return this.$store.getters[\"category/getAdded\"];\n    }\n  },\n  methods: {\n    ...mapActions('category', ['setAddedCtg']),\n    ...mapActions('category', ['setHistoryOptionCtg']),\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    ...mapActions(\"category\", [\"getInfoByIdCtg\"]),\n    submit: async function (name) {\n      this.status = '';\n      this.message = '';\n      let category = {\n        \"id\": this.added.id,\n        \"name\": name\n      };\n      let result = await this.$store.dispatch(\"category/editItemCtg\", {\n        option: 'category',\n        item: category\n      });\n      if (result.status == 'error') {\n        this.status = 'error';\n        this.message = result.message;\n      } else {\n        // success\n        this.showModal();\n      }\n    },\n    closeSelect() {\n      this.active = false;\n      document.getElementById(\"select-ch\").checked = false;\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n      this.status2 = \"\";\n      this.message2 = \"\";\n    },\n    showModal: function () {\n      this.viewModal = true;\n    },\n    closeModal: function () {\n      this.viewModal = false;\n      this.setAddedCtg('');\n      this.setHistoryOptionCtg('Default');\n    },\n    Edit: function () {\n      this.viewModal = false;\n    },\n    back: function () {\n      setTimeout(() => this.$router.go(), 200);\n    }\n  }\n};","map":{"version":3,"mappings":"AAmEA;AACA;EACAA;EAEAC;IACA;MACAC;MACAC;MACAC;MAEAC;IAEA;EACA;EAEAC;IACAC;MACA;IACA;EAGA;EACAC;IAEA;IACA;IACAC;MACAC;IACA;IACA;IACAC;MACA;MACA;MACA;QACA;QACA;MAIA;MAIA;QAAAC;QAAAC;MAAA;MAEA;QACA;QACA;MACA;QAAA;QACA;MAEA;IAIA;IACAC;MACA;MACAC;IACA;IACAC;MACAN,iCACA;IAGA;IACAO;MACA;MACA;MACA;MACA;IACA;IAGAC;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;IACA;IACAC;MACAX;IACA;EAGA;AAGA","names":["name","data","status","message","active","viewModal","computed","added","methods","wait","setTimeout","submit","option","item","closeSelect","document","delStatus","delMsgs","showModal","closeModal","Edit","back"],"sourceRoot":"src/views/category","sources":["editCtg.vue"],"sourcesContent":["<template >\r\n    <div id=\"EditCtg\">\r\n               \r\n               <div class=\"form-content\">\r\n            <form @submit.prevent=\"\r\n                submit(\r\n                 added.name \r\n                )\r\n              \">\r\n                   \r\n                     <div class=\"grup-form dflx\">\r\n                        <div class=\"boxform\">\r\n                            <label for=\"name\" class=\"name\">Nombre </label>\r\n                            <input v-model=\"added.name\" type=\"text\" id=\"name\" name=\"name\"    required class=\"in1\">\r\n                        </div>\r\n                      \r\n                      \r\n                        \r\n                    </div>\r\n                   \r\n                \r\n                   \r\n                      <div class=\"dflx\">\r\n                               <p @click=\"wait()\" class=\"btnRegresar\">Regresar</p>\r\n                               <button class=\"alta\" type=\"submit\">Editar </button>\r\n                        </div>\r\n\r\n                    \r\n                        <div v-if=\"status !=''\" class=\"status_messages\">\r\n                            <div v-if=\"status =='success'\" class=\"msg msg_success\">\r\n                                 <p>{{message}}</p>\r\n                            </div>\r\n                            <div  v-if=\"status =='error'\" class=\"msg msg_error\">\r\n                                <p>{{message}}</p>\r\n                            </div>\r\n                        </div>\r\n            </form>\r\n\r\n\r\n                     \r\n          \r\n\r\n\r\n        </div>\r\n\r\n               \r\n\r\n                \r\n            <div v-if=\"viewModal\" class=\"modal_view\">\r\n           \r\n            <div id=\"modal_edit\">\r\n                <div class=\"body dflx\">\r\n                     \r\n                           <h3>Categor√≠a actualizada </h3>\r\n                           <img src=\"../../assets/default/add.png\" alt=\"icono alerta\">\r\n                </div>\r\n           \r\n            <div class=\"modal_edit_btns dflx\">\r\n                <p @click=\"wait()\" class=\"otro\">Aceptar</p> <!-- <p  @click=\"back()\" class=\"inicio\">Inicio</p>-->\r\n            </div>\r\n          </div>\r\n        </div> \r\n\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nimport {mapActions} from 'vuex';\r\nexport default {\r\n    name:'EditCtg',\r\n    \r\n    data(){\r\n        return{\r\n           status:'',\r\n           message:'',\r\n            active: false,\r\n          \r\n            viewModal:false,\r\n          \r\n        }\r\n    },\r\n   \r\n    computed:{\r\n        added(){\r\n            return this.$store.getters[\"category/getAdded\"]\r\n        },\r\n        \r\n        \r\n    },\r\n    methods:{\r\n        \r\n          ...mapActions('category', ['setAddedCtg']),\r\n         ...mapActions('category', ['setHistoryOptionCtg']),\r\n          wait: function(){\r\n            setTimeout(() => this.$router.go(), 200);\r\n        },\r\n        ...mapActions(\"category\", [\"getInfoByIdCtg\"]),\r\n             submit: async function(  name  ){\r\n                 this.status =''\r\n                 this.message =''\r\n            let category ={\r\n                    \"id\":this.added.id,\r\n                    \"name\":name,\r\n                \r\n                   \r\n                   \r\n                    }\r\n\r\n                   \r\n\r\n                        let result = await this.$store.dispatch(\"category/editItemCtg\",  {option:'category', item: category});\r\n                    \r\n                        if(result.status=='error'){\r\n                            this.status='error'\r\n                            this.message= result.message\r\n                        }else{ // success\r\n                         this.showModal()\r\n                          \r\n                        } \r\n             \r\n\r\n            \r\n    },\r\n   closeSelect() {\r\n      this.active = false;\r\n      document.getElementById(\"select-ch\").checked = false;\r\n    },\r\n     delStatus: function () {\r\n            setTimeout(() => this.delMsgs()\r\n            ,2000);\r\n           \r\n            \r\n        },\r\n        delMsgs: function(){\r\n            this.status=\"\"\r\n            this.message=\"\"\r\n             this.status2=\"\"\r\n            this.message2=\"\"\r\n        },\r\n\r\n\r\n      showModal: function(){\r\n         this.viewModal=true\r\n     },\r\n    closeModal:function(){\r\n        this.viewModal=false;\r\n        this.setAddedCtg('')\r\n        this.setHistoryOptionCtg('Default')\r\n    },\r\n    Edit:function(){\r\n        this.viewModal=false;\r\n    },\r\n    back:function(){\r\n       setTimeout(() => this.$router.go(), 200);\r\n    },\r\n\r\n\r\n   }\r\n\r\n\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n  \r\n   \r\n    #EditCtg label{\r\n        color: var(--color-2);\r\n        font-weight: 700;\r\n        font-size: 0.7291666666666666VW;\r\n        margin-bottom: .3vw;\r\n    }\r\n\r\n    #EditCtg input, #EditCtg select{\r\n        height: 2.5VW;\r\n        width: 14.716666666666668VW;\r\n        border: 0.052083vw solid var(--color-2);\r\n        border-radius: 0vw  1.5vw 1.5vw  0vw ;\r\n        color: var(--color-4);\r\n        font-size: 0.7291666666666666VW;\r\n        font-weight: 500;\r\n        padding-left: 1.09375VW;\r\n    }\r\n\r\n\r\n