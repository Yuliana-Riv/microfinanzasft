{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions } from \"vuex\";\n/**/\nimport AddCar from \"./AddCupon.vue\";\nimport EditCar from \"./EditCupon.vue\";\nexport default {\n  name: \"Cupones\",\n  components: {\n    /**/\n    AddCar,\n    EditCar\n  },\n  data() {\n    return {\n      status: \"\",\n      message: \"\",\n      item: \"\",\n      search_value: \"\",\n      updsearchcli: \"\",\n      currentPage: 1,\n      itemsPerPage: 12,\n      active: false,\n      viewModal: false,\n      itemDel: \"\",\n      radius: false\n    };\n  },\n  created() {\n    this.getAllInfoCar(\"cupones\");\n  },\n  computed: {\n    data() {\n      let arr = this.$store.getters[\"shopcar/data\"];\n      let filter = [];\n      if (!Array.isArray(arr)) {\n        return [];\n      } else {\n        if (this.search_value != '') {\n          for (const item of arr) {\n            if (this.includesItem(this.search_value, item.name)) {\n              filter.push(item);\n            }\n          }\n        } else {\n          filter = arr;\n        }\n      }\n      return filter;\n    },\n    historyOption() {\n      return this.$store.getters[\"shopcar/getHistoryOption\"];\n    },\n    //Paginacion\n    allItems() {\n      return Object.keys(this.data).map(pid => this.data[pid]);\n    },\n    filteredItems() {\n      return this.allItems.slice((this.currentPage - 1) * this.itemsPerPage, this.currentPage * this.itemsPerPage);\n    },\n    totalPages() {\n      let total = Math.ceil(this.allItems.length / this.itemsPerPage);\n      if (total < this.currentPage) {\n        this.currentPage = 1;\n      }\n      return total;\n    }\n  },\n  methods: {\n    ...mapActions(\"shopcar\", [\"getAllInfoCar\"]),\n    ...mapActions(\"shopcar\", [\"setHistoryOptionCar\"]),\n    ...mapActions(\"shopcar\", [\"setAddedCar\"]),\n    changeView(option, item) {\n      this.item = item;\n      this.setAddedCar(item);\n      this.setHistoryOptionCar(option);\n    },\n    includesItem(search, name) {\n      if (search == \"\") {\n        return true;\n      }\n      name = name.toLowerCase();\n      search = search.toLowerCase();\n      if (search == \"\") {\n        return false;\n      }\n      return name.includes(search);\n    },\n    activeRadius() {\n      this.radius ? this.radius = false : this.radius = true;\n    },\n    showModal: function (id) {\n      this.itemDel = id;\n      this.viewModal = true;\n    },\n    closeModal: function () {\n      this.viewModal = false;\n      this.itemDel = \"\";\n    },\n    wait: function () {\n      setTimeout(() => this.$router.go(), 200);\n    },\n    delStatus: function () {\n      setTimeout(() => this.delMsgs(), 2000);\n      setTimeout(() => this.wait(), 500);\n    },\n    delMsgs: function () {\n      this.status = \"\";\n      this.message = \"\";\n    },\n    eliminar: async function () {\n      let id = this.itemDel;\n      this.status = \"\";\n      this.message = \"\";\n      let result = await this.$store.dispatch(\"main/deleteItem\", {\n        option: \"cupon\",\n        id: id\n      });\n      if (result.status == \"error\") {\n        this.message = result.message;\n        this.status = \"error\";\n        this.delStatus();\n      } else {\n        this.status = \"\";\n        this.message = \"\";\n        this.wait();\n      }\n    },\n    // botones de paginacion.\n    next() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prev() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAsNA;AACA;AACA;AACA;AACA;EACAA;EACAC;IACA;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EAGAC;IACAb;MACA;MACA;MACA;QACA;MACA;QACA;UACA;YACA;cACAc;YACA;UACA;QACA;UACAA;QACA;MACA;MAEA;IACA;IACAC;MACA;IACA;IAEA;IACAC;MACA;IACA;IACAC;MACA,2BACA,4CACA,qCACA;IACA;IACAC;MACA;MACA;QACA;MACA;MACA;IACA;EACA;EACAC;IACA;IACA;IACA;IAEAC;MACA;MACA;MACA;IACA;IACAC;MACA;QACA;MACA;MACAzB;MACA0B;MACA;QACA;MACA;MACA;IACA;IAGAC;MACA;IACA;IAEAC;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IAEAC;MACAC;IACA;IAEAC;MACAD;MACAA;IACA;IACAE;MACA;MACA;IACA;IAEAC;MACA;MACA;MACA;MAEA;QACAC;QACAC;MACA;MAEA;QACA;QACA;QACA;MACA;QACA;QACA;QACA;MACA;IACA;IAGA;IACAC;MACA;QACA;MACA;IACA;IACAC;MACA;QACA;MACA;IACA;EACA;AACA","names":["name","components","AddCar","EditCar","data","status","message","item","search_value","updsearchcli","currentPage","itemsPerPage","active","viewModal","itemDel","radius","created","computed","filter","historyOption","allItems","filteredItems","totalPages","methods","changeView","includesItem","search","activeRadius","showModal","closeModal","wait","setTimeout","delStatus","delMsgs","eliminar","option","id","next","prev"],"sourceRoot":"src/views/shopcar","sources":["cupon.vue"],"sourcesContent":["<template>\r\n  <div id=\"Categorias\">\r\n    \r\n    <div v-if=\"historyOption == 'Default'\">\r\n      <div class=\"dflx\">\r\n        <p class=\"noitems\">\r\n          No. de items\r\n          <span v-if=\"data.length>0\">{{\r\n            data.length\r\n          }}</span>\r\n          <span v-else>0</span>\r\n        </p>\r\n        <div class=\"select-container\">\r\n          <input\r\n            type=\"checkbox\"\r\n            name=\"select-ch\"\r\n            id=\"select-ch\"\r\n            v-model=\"active\"\r\n            true-value=\"true\"\r\n            false-value=\"false\"\r\n          />\r\n          <label for=\"select-ch\">\r\n            <div id=\"select-items\" :class=\"{ rounded: active }\">\r\n              Items por página: {{ itemsPerPage }}\r\n            </div>\r\n          </label>\r\n          <div class=\"options-cont\" @click=\"closeSelect()\">\r\n            <div\r\n              class=\"option-btn\"\r\n              v-bind:value=\"6\"\r\n              @click=\"itemsPerPage = '6'\"\r\n            >\r\n              6\r\n            </div>\r\n            <div\r\n              class=\"option-btn\"\r\n              v-bind:value=\"12\"\r\n              @click=\"itemsPerPage = '12'\"\r\n            >\r\n              12\r\n            </div>\r\n            <div\r\n              class=\"option-btn\"\r\n              v-bind:value=\"18\"\r\n              @click=\"itemsPerPage = '18'\"\r\n            >\r\n              18\r\n            </div>\r\n            <div\r\n              class=\"option-btn\"\r\n              v-bind:value=\"24\"\r\n              @click=\"itemsPerPage = '24'\"\r\n            >\r\n              24\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"buscador dflx\">\r\n          <input\r\n            v-model=\"search_value\"\r\n            type=\"search\"\r\n            id=\"input-search\"\r\n            class=\"form-control\"\r\n            :placeholder=\"'Búsqueda'\"\r\n          />\r\n          <div class=\"icon_buscar\">\r\n            <img\r\n             \r\n              src=\"../../assets/Search.png\"\r\n              alt=\"icono buscar\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div @click=\"changeView('AddCou', '')\" class=\"btn_añadir dflx\">\r\n        <p>AGREGAR</p>\r\n        <img src=\"../../assets/add.png\" alt=\"\" />\r\n      </div>\r\n\r\n      <div class=\"tbl_categorias\">\r\n        <div class=\"tbl_header dflx\">\r\n          <div class=\"tblh tblrad\">\r\n            <p>CUPÓN</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>CÓDIGO</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>ESTATUS</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>TIPO</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>DISPONIBLES</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>IAT</p>\r\n          </div>\r\n          <div class=\"tblh\">\r\n            <p>EXP</p>\r\n          </div>\r\n          <div class=\"tblh2\">\r\n            <p>EDITAR</p>\r\n          </div>\r\n          <div class=\"tblh2 tblh2r\">\r\n            <p>BORRAR</p>\r\n          </div>\r\n        </div>\r\n        <div tbl_content>\r\n          <div\r\n            v-if=\" data.length>0\">\r\n            <div\r\n              class=\"tbl_items dflx\"\r\n              v-for=\"(item, key) in filteredItems\"\r\n              :key=\"key\"\r\n            >\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.name }}</p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.code }}</p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.status }}</p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.type }}</p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.totalAvailable }}</p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.date_a }}</p>\r\n              </div>\r\n              <div class=\"tbl_item\">\r\n                <p>{{ item.date_b }}</p>\r\n              </div>\r\n\r\n              <div class=\"icon_editar\">\r\n                <img\r\n                  @click=\"changeView('EditCou', item)\"\r\n                  src=\"../../assets/i_editar.png\"\r\n                  alt=\"icono editar\"\r\n                />\r\n              </div>\r\n              <div class=\"icon_eliminar\">\r\n                <img\r\n                  @click=\"showModal(item.id)\"\r\n                  src=\"../../assets/i_eliminar.png\"\r\n                  alt=\"icono eliminar\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div v-else>\r\n            <p class=\"snu\">Aun no hay registros.</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div id=\"navigate-buttons\" class=\"mlf\">\r\n        <div class=\"dflx\">\r\n          <button @click=\"prev\" id=\"btn-prev\" class=\"btn-navigate\">\r\n            <img src=\"../../assets/Left.png\" alt=\"icono anterior\" />\r\n          </button>\r\n          <p class=\"num-pages\">{{ currentPage }} / {{ totalPages }}</p>\r\n          <button @click=\"next\" id=\"btn-next\" class=\"btn-navigate\">\r\n            <img src=\"../../assets/Right.png\" alt=\"icono siguiente\" />\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div v-if=\"historyOption == 'AddCou'\">\r\n      <!-- Agregar categorias -->\r\n      <AddCar />\r\n    </div>\r\n    <div v-if=\"historyOption == 'EditCou'\">\r\n      <!-- Editar categorias -->\r\n      <EditCar />\r\n    </div>\r\n\r\n    <div v-if=\"status != ''\" class=\"status_messages\">\r\n      <div v-if=\"status == 'success'\" class=\"msg msg_success\">\r\n        <p>{{ message }}</p>\r\n      </div>\r\n      <div v-if=\"status == 'error'\" class=\"msg msg_error\">\r\n        <p>{{ message }}</p>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-if=\"viewModal\" class=\"modal_view\">\r\n      <div id=\"modal_delete\">\r\n        <div class=\"body\">\r\n          <img src=\"../../assets/alert.png\" alt=\"icono alerta\" />\r\n          <h3>\r\n            ¿Estás seguro de\r\n            <span\r\n              >eliminar <br />\r\n              este cupón?</span\r\n            >\r\n          </h3>\r\n        </div>\r\n\r\n        <div class=\"modal_del_btns dflx\">\r\n          <p @click=\"eliminar()\">Aceptar</p>\r\n          <p @click=\"closeModal()\">Cancelar</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { mapActions } from \"vuex\";\r\n/**/\r\nimport AddCar from \"./AddCupon.vue\";\r\nimport EditCar from \"./EditCupon.vue\";\r\nexport default {\r\n  name: \"Cupones\",\r\n  components: {\r\n    /**/\r\n    AddCar,\r\n    EditCar,\r\n  },\r\n  data() {\r\n    return {\r\n      status: \"\",\r\n      message: \"\",\r\n      item: \"\",\r\n      search_value: \"\",\r\n      updsearchcli: \"\",\r\n      currentPage: 1,\r\n      itemsPerPage: 12,\r\n      active: false,\r\n      viewModal: false,\r\n      itemDel: \"\",\r\n      radius: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.getAllInfoCar(\"cupones\");\r\n  },\r\n\r\n\r\n  computed: {\r\n    data() {\r\n      let arr =this.$store.getters[\"shopcar/data\"];\r\n          let filter =[]\r\n              if(!Array.isArray(arr)){\r\n                return []\r\n              }else{\r\n                  if(this.search_value !=''){\r\n                    for (const item of arr) {\r\n                        if(this.includesItem(this.search_value, item.name)){\r\n                          filter.push(item)\r\n                        }\r\n                    }\r\n                  }else{\r\n                    filter = arr\r\n                  }\r\n              }\r\n              \r\n                return filter\r\n    },\r\n    historyOption() {\r\n      return this.$store.getters[\"shopcar/getHistoryOption\"];\r\n    },\r\n\r\n    //Paginacion\r\n    allItems() {\r\n      return Object.keys(this.data).map((pid) => this.data[pid]);\r\n    },\r\n    filteredItems() {\r\n      return this.allItems.slice(\r\n        (this.currentPage - 1) * this.itemsPerPage,\r\n        this.currentPage * this.itemsPerPage\r\n      );\r\n    },\r\n    totalPages() {\r\n      let total = Math.ceil(this.allItems.length / this.itemsPerPage);\r\n      if (total < this.currentPage) {\r\n        this.currentPage = 1;\r\n      }\r\n      return total;\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions(\"shopcar\", [\"getAllInfoCar\"]),\r\n    ...mapActions(\"shopcar\", [\"setHistoryOptionCar\"]),\r\n    ...mapActions(\"shopcar\", [\"setAddedCar\"]),\r\n\r\n    changeView(option, item) {\r\n      this.item = item;\r\n      this.setAddedCar(item);\r\n      this.setHistoryOptionCar(option);\r\n    },\r\n    includesItem(search, name) {\r\n            if (search == \"\") {\r\n              return true;\r\n            }\r\n            name = name.toLowerCase();\r\n            search = search.toLowerCase();\r\n            if (search == \"\") {\r\n              return false;\r\n            }\r\n            return name.includes(search);\r\n         },\r\n\r\n\r\n    activeRadius() {\r\n      this.radius ? (this.radius = false) : (this.radius = true);\r\n    },\r\n\r\n    showModal: function (id) {\r\n      this.itemDel = id;\r\n      this.viewModal = true;\r\n    },\r\n    closeModal: function () {\r\n      this.viewModal = false;\r\n      this.itemDel = \"\";\r\n    },\r\n\r\n    wait: function () {\r\n      setTimeout(() => this.$router.go(), 200);\r\n    },\r\n\r\n    delStatus: function () {\r\n      setTimeout(() => this.delMsgs(), 2000);\r\n      setTimeout(() => this.wait(), 500);\r\n    },\r\n    delMsgs: function () {\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n    },\r\n\r\n    eliminar: async function () {\r\n      let id = this.itemDel;\r\n      this.status = \"\";\r\n      this.message = \"\";\r\n      \r\n      let result = await this.$store.dispatch(\"main/deleteItem\", {\r\n        option: \"cupon\",\r\n        id: id,\r\n      });\r\n\r\n      if (result.status == \"error\") {\r\n        this.message = result.message;\r\n        this.status = \"error\";\r\n        this.delStatus();\r\n      } else {\r\n        this.status = \"\";\r\n        this.message = \"\";\r\n        this.wait();\r\n      }\r\n    },\r\n\r\n\r\n    // botones de paginacion.\r\n    next() {\r\n      if (this.currentPage < this.totalPages) {\r\n        this.currentPage++;\r\n      }\r\n    },\r\n    prev() {\r\n      if (this.currentPage > 1) {\r\n        this.currentPage--;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.tbl_categorias {\r\n  margin-top: 1.9791666666666665vw;\r\n  width: 100%;\r\n  /** width: 26.8VW; */\r\n}\r\n\r\n.tbl_items {\r\n  border-bottom: 1px solid 